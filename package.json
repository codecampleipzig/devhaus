{
  "name": "devhaus",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "install:functions": "cd functions && yarn install",
    "install:frontend": "cd frontend && yarn install",
    "postinstall": "yarn install:functions && yarn install:frontend",
    "start:emulators": "firebase emulators:start",
    "start:frontend": "cd frontend && yarn serve",
    "lint:functions": "cd functions && yarn lint --fix",
    "lint:frontend": "cd frontend && yarn lint --fix",
    "lint": "yarn lint:functions && yarn lint:frontend",
    "test:e2e": "cd frontend && yarn test:e2e",
    "test:unit": "cd frontend && yarn test:unit",
    "test:ci": "yarn test:unit && yarn test:e2e --headless",
    "test": "yarn test:unit && yarn test:e2e"
  },
  "devDependencies": {},
  "dependencies": {
    "email-existence": "^0.1.6"
  }
}
